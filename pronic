import java.util.*;
public class Main
{
	public static void main(String[] args) {
	    Scanner sc=new Scanner(System.in);
	    int n=sc.nextInt();
	    boolean isPronic=false;
	    for(int i=1;i<=n- 1;i++){
	        if(i* (i + 1)==n){
	            isPronic=true;
	            break;
	        }
	    }
	        if(isPronic){
	            System.out.println("Pronic");
	        }
	        else{
	        System.out.println("Not");
	    }
	    
	
	}
